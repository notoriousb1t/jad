{
  "author": "christopher.j.wallis@gmail.com",
  "bugs": {
    "url": "https://github.com/notoriousb1t/empty-tsproj/issues"
  },
  "dependencies": {
    "angular": "^1.5.6",
    "es6-shim": "^0.35.0",
    "golden-layout": "^1.5.1",
    "just-animate": "^0.6.0",
    "redux": "^3.5.2",
    "reflect-metadata": "0.1.2",
    "systemjs": "0.19.24",
    "zone.js": "0.6.6"
  },
  "devDependencies": {
    "autoless": "^0.1.7",
    "concurrently": "^2.0.0",
    "copyfiles": "^0.2.1",
    "less": "^2.6.1",
    "less-plugin-clean-css": "^1.5.1",
    "lite-server": "^2.1.0",
    "tslint": "^3.10.2",
    "typescript": "^1.8.9",
    "typings": "^0.7.9"
  },
  "description": "Just Animate Designer",
  "keywords": [
    "web animation",
    "animation",
    "easing"
  ],
  "name": "just-animate-designer",
  "repository": {
    "type": "git",
    "url": "https://github.com/notoriousb1t/just-animate-designer"
  },
  "scripts": {
    "styles": "autoless --autoprefix \"last 2 versions\" ./wwwroot/styles",
    "start": "concurrently \"npm run tsc:w\" \"npm run lite\" \"npm run styles\"",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "lite": "lite-server",
    "typings": "typings",
    "postinstall": "typings install && npm run copylibs",
    "copylibs": "copyfiles -u 1 \"node_modules/redux/dist/**/*\" \"node_modules/just-animate/**/*\" \"node_modules/angular/**/*\" \"node_modules/jquery/**/*\" \"node_modules/golden-layout/**/*\" \"node_modules/es6-shim/*.min.js\" \"node_modules/systemjs/dist/*\" \"./wwwroot/lib\""
  },
  "version": "0.0.1a"
}
